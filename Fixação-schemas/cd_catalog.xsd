<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="catalog">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="cd" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="title" type="xs:string" maxOccurs="unbounded" />
                            <xs:element name="artist" type="xs:string" maxOccurs="unbounded" />
                            <xs:element name="country" type="xs:string" maxOccurs="unbounded" />
                            <xs:element name="company" type="xs:string" maxOccurs="unbounded" />
                            <xs:element name="price" type="xs:decimal" maxOccurs="unbounded" />
                            <xs:element name="year" type="xs:integer" maxOccurs="unbounded" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>